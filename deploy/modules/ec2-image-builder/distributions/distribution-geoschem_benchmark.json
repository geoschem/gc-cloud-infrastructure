{
	"name": "distribution-geoschem_benchmark",
	"description": "Distributes container image to Amazon ECR repository.",
	"distributions": [
	    {
	        "region": "us-east-1",
	        "containerDistributionConfiguration": {
	            "description": "GEOS-Chem benchmarking dependencies.",
	            "targetRepository": {
	                "service": "ECR",
	                "repositoryName": "geoschem_benchmark"
	            },
	            "containerTags": ["latest", "{{imagebuilder:buildDate}}"]
	        }
	    }
	]
}
