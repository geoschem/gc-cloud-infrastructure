{
	"name": "distribution-geoschem_benchmark-<BUILD_TAG>",
	"description": "Distributes container image to Amazon ECR repository.",
	"distributions": [
	    {
	        "region": "us-east-1",
	        "containerDistributionConfiguration": {
	            "description": "GEOS-Chem benchmarking dependencies.",
	            "targetRepository": {
	                "service": "ECR",
	                "repositoryName": "public.ecr.aws/geoschem-wustl/geoschem_benchmark"
	            },
	            "containerTags": ["latest-<BUILD_TAG>", "{{imagebuilder:buildDate}}-<BUILD_TAG>"]
	        }
	    }
	]
}
