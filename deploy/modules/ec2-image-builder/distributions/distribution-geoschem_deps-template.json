{
	"name": "distribution-geoschem_deps-<BUILD_TAG>",
	"description": "Distributes container image to Amazon ECR repository.",
	"distributions": [
	    {
	        "region": "us-east-1",
	        "containerDistributionConfiguration": {
	            "description": "GEOS-Chem dependencies.",
	            "targetRepository": {
	                "service": "ECR",
	                "repositoryName": "geoschem_deps"
	            },
	            "containerTags": ["latest-<BUILD_TAG>", "{{imagebuilder:buildDate}}-<BUILD_TAG>"]
	        }
	    }
	]
}
